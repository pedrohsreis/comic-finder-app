{
  "name": "MarvelApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "initial-setup-android": "npm install && react-native run-android",
    "initial-setup-ios": "npm install && cd ios && pod install && cd .. && react-native run-ios"
  },
  "dependencies": {
    "@react-native-community/viewpager": "^5.0.11",
    "@react-native-masked-view/masked-view": "^0.2.2",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.14.3",
    "axios": "^0.21.1",
    "md5": "^2.3.0",
    "react": "16.13.1",
    "react-dom": "^17.0.1",
    "react-native": "0.63.4",
    "react-native-elements": "^3.2.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-paper": "^4.7.2",
    "react-native-reanimated": "^2.0.0",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.18.1",
    "react-native-vector-icons": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/runtime": "^7.13.9",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.21.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.65.2",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "./jest-setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/fileTransformer.js",
      "\\.(css|less)$": "<rootDir>/tests/fileTransformer.js"
    }
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
  }
}
